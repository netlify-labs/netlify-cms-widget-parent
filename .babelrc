{
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": [
          "last 2 Chrome versions",
          "last 2 Opera versions",
          "last 2 Firefox versions",
          "last 2 Edge versions",
          "last 2 Safari versions",
          "last 2 iOS versions",
          "last 2 ChromeAndroid versions"
        ]
      }
    ],
    "@babel/preset-react"
  ],
  "plugins": ["emotion", "@babel/plugin-proposal-class-properties", "@babel/plugin-proposal-optional-chaining"]
}
