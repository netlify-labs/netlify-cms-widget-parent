    "eslint": "^8.0.0",
    "eslint-config-prettier": "^8.0.0",
    "eslint-plugin-react": "^7.20.3",
    "html-webpack-plugin": "^5.0.0",
    "husky": "^4.2.5",
    "jest": "^27.0.0",
    "npm-run-all2": "^5.0.0",
    "path-browserify": "^1.0.1",
    "prettier": "^2.0.5",
    "webpack": "^5.11.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
