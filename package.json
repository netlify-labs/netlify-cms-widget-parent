{
  "name": "@netlify/netlify-cms-widget-parent",
  "version": "1.0.5",
  "description": "A Netlify CMS widget to select directory parent for an entry",
  "main": "dist/netlify-cms-widget-parent.js",
  "scripts": {
    "build": "webpack --env.production",
    "develop": "webpack-dev-server --env.development",
    "lint": "eslint 'src/**/*.js'",
    "format": "prettier --write 'src/**/*.js'",
    "format:ci": "prettier --check 'src/**/*.js'",
    "semantic-release": "cross-env CI=true HUSKY_SKIP_HOOKS=1 semantic-release",
    "release": "run-s build semantic-release && npm publish"
  },
  "files": [
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/netlify-labs/netlify-cms-widget-parent.git"
  },
  "keywords": [
    "netlify",
    "cms",
    "widget"
  ],
  "author": "netlify-labs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/netlify-labs/netlify-cms-widget-parent/issues"
  },
  "homepage": "https://github.com/netlify-labs/netlify-cms-widget-parent#readme",
  "dependencies": {
    "@emotion/core": "^10.0.35",
    "@emotion/styled": "^10.0.27",
    "netlify-cms-ui-default": "^2.11.1",
    "react-select": "^3.1.0",
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/preset-react": "^7.10.1",
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-emotion": "^10.0.33",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-react": "^7.20.3",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "semantic-release": "^17.1.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
